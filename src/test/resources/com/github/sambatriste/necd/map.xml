<component-configuration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://tis.co.jp/nablarch/component-configuration"
        xsi:schemaLocation="http://tis.co.jp/nablarch/component-configuration  https://nablarch.github.io/schema/component-configuration.xsd">

  <map name="literalMap">
    <entry key="foo" value="bar"/>
    <entry key="hoge" value="fuga"/>
  </map>

  <map name="componentMap">
    <entry key="buz">
      <value-component class="com.github.sambatriste.necd.PlainObject">
        <property name="name" value="aaa"/>
      </value-component>
    </entry>
  </map>

</component-configuration>
